<?xml version="1.0" encoding="UTF-8"?>
<con:businessServiceEntry xmlns:con="http://xmlns.oracle.com/servicebus/business/config">
    <con:coreEntry>
        <con:description>Publish select log events to Kafka</con:description>
        <con1:binding type="Mixed" xsi:type="con:MixedBindingType" xmlns:con="http://www.bea.com/wli/sb/services/bindings/config" xmlns:con1="http://xmlns.oracle.com/servicebus/business/config" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <con:request type="Text"/>
        </con1:binding>
        <oper:operations enabled="true" xmlns:oper="http://xmlns.oracle.com/servicebus/business/operations">
            <oper:throttling enabled="false"/>
            <oper:resultCachingEnabled>true</oper:resultCachingEnabled>
        </oper:operations>
        <con:xqConfiguration>
            <ser:snippetVersion xmlns:ser="http://www.bea.com/wli/sb/services">1.0</ser:snippetVersion>
        </con:xqConfiguration>
    </con:coreEntry>
    <con:endpointConfig>
        <tran:provider-id xmlns:tran="http://www.bea.com/wli/sb/transports">kafka</tran:provider-id>
        <tran:inbound xmlns:tran="http://www.bea.com/wli/sb/transports">false</tran:inbound>
        <tran:URI xmlns:tran="http://www.bea.com/wli/sb/transports">
            <env:value xmlns:env="http://www.bea.com/wli/config/env">localhost:9093</env:value>
        </tran:URI>
        <tran:URI xmlns:tran="http://www.bea.com/wli/sb/transports">
            <env:value xmlns:env="http://www.bea.com/wli/config/env">localhost:9094</env:value>
        </tran:URI>
        <tran:outbound-properties xmlns:tran="http://www.bea.com/wli/sb/transports">
            <tran:load-balancing-algorithm>none</tran:load-balancing-algorithm>
            <tran:retry-count>0</tran:retry-count>
            <tran:retry-interval>0</tran:retry-interval>
        </tran:outbound-properties>
        <tran:provider-specific xsi:type="kaf:KafkaEndPointConfiguration" xmlns:kaf="http://oracle/ateam/sb/transports/kafka" xmlns:tran="http://www.bea.com/wli/sb/transports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <kaf:topic-name>osb-logevents-topic</kaf:topic-name>
            <kaf:request-type>TEXT</kaf:request-type>
            <kaf:response-type>NONE</kaf:response-type>
            <kaf:custom-props>ClientIdentifier=SBAuditLogger</kaf:custom-props>
            <kaf:outbound-properties>
                <kaf:acks>1</kaf:acks>
                <kaf:timeout-ms>30000</kaf:timeout-ms>
            </kaf:outbound-properties>
        </tran:provider-specific>
    </con:endpointConfig>
</con:businessServiceEntry>